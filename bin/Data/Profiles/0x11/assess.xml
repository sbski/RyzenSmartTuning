<dc_configuration>

  <ebp name="Assess Performance" mux_period="1">

      <event select="C0" mask="00" os="T" user="T" count="250000"></event>
      <event select="76" mask="00" os="T" user="T" count="250000"></event>
      <event select="C2" mask="00" os="T" user="T" count="25000"></event>
      <event select="C3" mask="00" os="T" user="T" count="25000"></event>
      <event select="40" mask="00" os="T" user="T" count="250000"></event>
      <event select="41" mask="00" os="T" user="T" count="25000"></event>
      <event select="46" mask="00" os="T" user="T" count="25000"></event>
      <event select="47" mask="00" os="T" user="T" count="25000"></event>

    <tool_tip>
      Get an overall assessment of performance
    </tool_tip>

    <description>
      Use this configuration to get an overall assessment of performance and
      to find potential issues for investigation.
    </description>

  </ebp>

</dc_configuration>

<?xml version="1.0"?>
<!--
    IBS load/store op summary view configuration.

    Related DC configurations: ibs.xml

    Family : 17h
    Model  : 00h-0Fh
    Flavor : Public
    Copyright (c) 2018-2019 Advanced Micro Devices, Inc.
-->
<view_configuration>
    <view name="IBS MEM all load/store" separate_cpus="F" separate_processes="F" separate_threads="F">
        <data>
            <event id="OpLoadStore" select="F200" mask="00"/>
            <event id="OpLoad" select="F201" mask="00"/>
            <event id="OpStore" select="F202" mask="00"/>
            <event id="OpDcMiss" select="F206" mask="00"/>
            <event id="OpLoadLat" select="F219" mask="00"/>
        </data>
        <output>
            <column title="IBS load/store" sort="descending">
                <value id="OpLoadStore"/>
            </column>
            <column title="IBS load" sort="none">
                <value id="OpLoad"/>
            </column>
            <column title="IBS store" sort="none">
                <value id="OpStore"/>
            </column>
            <column title="IBS DC miss" sort="none" visible="F">
                <value id="OpDcMiss"/>
            </column>
            <column title="IBS DC miss lat" sort="none" visible="F">
                <value id="OpLoadLat"/>
            </column>
            <column title="Ave DC miss lat" sort="none">
                <ratio left="OpLoadLat" right="OpDcMiss"/>
            </column>
        </output>
        <tool_tip>Show summary of IBS load/store op data</tool_tip>
        <description>Use this view to show a summary of IBS load/store op data.</description>
    </view>
</view_configuration>
